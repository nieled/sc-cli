{
  "name": "sc-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier:check": "prettier --check \"src/**/*.{md,json,yml,html,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"src/**/*.{md,json,yml,html,js,ts,tsx,css,scss}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf build/*",
    "build": "yarn clean && webpack",
    "serve": "webpack serve",
    "test": "jest --coverage --logHeapUsage --maxWorkers=2",
    "test:watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:audits": "cypress open --config-file cypress-audits.json",
    "e2e:cypress": "cypress run --browser chrome",
    "e2e:cypress:headless": "yarn e2e:cypress --headless",
    "e2e:cypress:audits": "cypress run --browser chrome --config-file cypress-audits.json",
    "e2e:cypress:audits:headless": "yarn e2e:cypress --headless --config-file cypress-audits.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.4.0",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cypress": "^8.3.1",
    "cypress-audit": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "lighthouse": "^8.3.0",
    "mini-css-extract-plugin": "^2.2.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  }
}
